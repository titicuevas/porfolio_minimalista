---
interface Props {
  class?: string
}

const { class: className } = Astro.props
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width="24"
  height="24"
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
>
  <rect x="2" y="3" width="20" height="14" rx="2" ry="2" stroke-width="2"/>
  <line x1="8" y1="21" x2="16" y2="21" stroke-width="2"/>
  <line x1="12" y1="17" x2="12" y2="21" stroke-width="2"/>
  <polyline points="6,9 10,13 6,17" stroke-width="2"/>
  <line x1="10" y1="13" x2="16" y2="13" stroke-width="2"/>
</svg> 